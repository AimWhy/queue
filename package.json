{
  "name": "queue",
  "version": "4.5.1",
  "description": "asynchronous function queue with adjustable concurrency",
  "keywords": [
    "queue",
    "async",
    "asynchronous",
    "synchronous",
    "job",
    "task",
    "concurrency",
    "concurrent"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "inherits": "~2.0.0"
  },
  "devDependencies": {
    "@types/node": "*",
    "browserify": "^5.9.1",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.2",
    "standard": "^8.6.0",
    "tape": "^2.14.0",
    "tsd-check": "*",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "test": "standard && node test && tsd-check",
    "test-browser": "standard && browserify test/index.js > test/bundle.js && echo \"open test/index.html in your browser\"",
    "travis": "standard && istanbul cover test --report lcovonly && cat coverage/lcov.info | coveralls",
    "travis-ts": "tsc test/typescript.ts --out /dev/null && echo 'TypeScript compilation passed.'",
    "example": "node example",
    "lint": "standard"
  },
  "repository": "https://github.com/jessetane/queue.git",
